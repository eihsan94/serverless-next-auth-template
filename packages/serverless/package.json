{
  "name": "ihsan-pay-serverless",
  "version": "0.0.1",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc",
    "prestart": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "tslint '*.ts'",
    "setup": "npm-run-all -p db:migrate",
    "db:migrate": "serverless dynamodb migrate --stage local",
    "dev": "docker-compose down && docker-compose up -d && npm-run-all -p startLambda:local",
    "startLambda:local": "serverless offline --stage local",
    "deploy:dev": "serverless deploy --stage dev",
    "deploy:prd": "serverless deploy --stage prd",
    "removeInfra:dev": "serverless remove --stage dev"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.77",
    "@types/aws-sdk": "0.0.42",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/node": "^7.0.5",
    "aws-lambda": "^1.0.6",
    "npm-run-all": "^4.1.5",
    "serverless": "^2.48.0",
    "serverless-dynamodb-local": "^0.2.39",
    "serverless-http": "^2.7.0",
    "serverless-offline": "^7.0.0",
    "serverless-s3-local": "^0.6.18",
    "serverless-webpack": "^5.5.1",
    "ts-loader": "^9.2.3",
    "tslint": "^6.1.3",
    "tslint-config-standard": "^6.0.1",
    "typescript": "^4.3.5",
    "webpack": "^5.47.1"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "morgan": "^1.10.0",
    "uuid": "^3.1.0"
  }
}
